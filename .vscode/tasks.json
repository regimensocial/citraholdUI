{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: Clang build active file",
            "command": "/usr/bin/clang++",
            "args": [
                "${workspaceFolder}/src/main.cpp",
                "-o",
                "${workspaceFolder}/build/main",
                "-lncurses",
                "-I${workspaceFolder}/include",
                "-std=c++17",
                "`/Users/jamie/Downloads/wxWidgets-3.2.3/build-release/wx-config --cxxflags --libs`"
            ],
            "options": {
                "cwd": "${workspaceRoot}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        },
        {
            "type": "cppbuild",
            "label": "C/C++: clang build active file",
            "command": "/usr/bin/clang",
            "args": [
                "-fcolor-diagnostics",
                "-fansi-escape-codes",
                "-g",
                "${file}",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
